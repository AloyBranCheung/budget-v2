{
  "name": "budget-v2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "docker compose -f docker-compose.yml up -d --build && next dev",
    "docker:stop": "docker compose -f docker-compose.yml down",
    "migrate:data:create": "dotenv -e .env.local -- ./scripts/create_static_data_migration.sh",
    "migrate:data": "dotenv -e .env.local -- ./scripts/migrate.sh",
    "migrate:dev": "dotenv -e .env.local -- yarn prisma migrate dev",
    "psql:local": "psql -U local -h localhost -p 5432 local",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@prisma/client": "^5.13.0",
    "next": "14.2.2",
    "next-auth": "^4.24.7",
    "pino": "^9.0.0",
    "pino-pretty": "^11.0.0",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.2",
    "postcss": "^8",
    "prisma": "^5.13.0",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.7.2",
    "typescript": "^5"
  }
}
