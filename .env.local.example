# Prisma supports the native connection string format for PostgreSQL, MySQL, SQLite, SQL Server, MongoDB and CockroachDB.
# See the documentation for all the connection string options: https://pris.ly/d/connection-strings

DATABASE_URL="postgresql://local:local@localhost:5432/local"
# only needed for supabase integration with prisma https://supabase.com/partners/integrations/prisma
# https://github.com/prisma/prisma/issues/17338
DIRECT_URL=
NEXT_PUBLIC_BASE_URL="http://localhost:3000"

# For toggling auth0 user migration 
LOCAL_ENV=true 

# For tests with dates/timezones
TZ=UTC

# AUTH0
AUTH0_SECRET='use [openssl rand -hex 32] to generate a 32 bytes value'
AUTH0_BASE_URL='http://localhost:3000'
AUTH0_ISSUER_BASE_URL=''
AUTH0_CLIENT_ID=''
AUTH0_CLIENT_SECRET=''

# AUTH0 Managmenet API
AUTH0_MANAGEMENT_API_CLIENT_ID=''
AUTH0_MANAGEMENT_API_CLIENT_SECRET=''

# NEXT_AUTH
NEXTAUTH_URL='http://localhost:3000'
NEXTAUTH_SECRET='use [openssl rand -hex 32] to generate a 32 bytes value'

# TEST User
TEST_USER=''
TEST_PASS=''